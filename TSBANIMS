local Section = Tab:NewSection("Tatsumaki")
Section:NewButton("Terrible Tornado Player", "Tatsumaki Second Ult Move", function()
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
local player = game.Players.LocalPlayer
repeat wait() until player.Character.Humanoid
local humanoid = player.Character.Humanoid
local character = player.Character or player.CharacterAdded:Wait()
local UserInputService = game:GetService("UserInputService")


local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://17278415853"


local playAnim = humanoid:LoadAnimation(anim)
playAnim:Play()

game.Players.LocalPlayer.Character.Torso.GreenAura.Real.Enabled = false
game.Players.LocalPlayer.Character.Torso.GreenAura.Real2.Enabled = false
wait(0.8)
local effect = game.ReplicatedStorage.Resources.TerribleTornado.DragWind.DragWind.DustEmitter1:Clone()
effect.Parent = game.Players.LocalPlayer.Character.Torso
    for _, child in ipairs(effect:GetChildren()) do
        if child:IsA("ParticleEmitter") then -- Check if the child is a ParticleEmitter
            child:Emit(1) -- Emit 20 particles
        end
    end
local effect2 = game.ReplicatedStorage.Resources.TerribleTornado.DragWind.DragWind.DustEmitter1:Clone()
effect2.Parent = game.Players.LocalPlayer.Character.Torso
    for _, child in ipairs(effect2:GetChildren()) do
        if child:IsA("ParticleEmitter") then -- Check if the child is a ParticleEmitter
            child:Emit(1) -- Emit 20 particles
        end
    end
local effect3 = game.ReplicatedStorage.Resources.TerribleTornado.DragWind.DragWind.dashleft:Clone()
effect3.Parent = game.Players.LocalPlayer.Character.Torso
    for _, child in ipairs(effect3:GetChildren()) do
        if child:IsA("ParticleEmitter") then -- Check if the child is a ParticleEmitter
            child:Emit(1) -- Emit 20 particles
        end
    end
local effect4 = game.ReplicatedStorage.Resources.TerribleTornado.DragWind.DragWind.dashright:Clone()
effect4.Parent = game.Players.LocalPlayer.Character.Torso
    for _, child in ipairs(effect4:GetChildren()) do
        if child:IsA("ParticleEmitter") then -- Check if the child is a ParticleEmitter
            child:Emit(1) -- Emit 20 particles
        end
    end
wait(2.7)
effect:Destroy()
effect2:Destroy()
effect3:Destroy()
effect4:Destroy()
wait(0.8)
local charge = game.ReplicatedStorage.Resources.TerribleTornado.HandCharge.Part.Attachment:Clone()
charge.Parent = game.Players.LocalPlayer.Character["Right Arm"]
    for _, child in ipairs(charge:GetChildren()) do
        if child:IsA("ParticleEmitter") then -- Check if the child is a ParticleEmitter
            child:Emit(1) -- Emit 20 particles
        end
    end
game.Lighting.ClockTime = 0
wait(3.6)
charge:Destroy()

local hit2 = game.ReplicatedStorage.Resources.TerribleTornado.ohye.Ohye.Attachment:Clone()
hit2.Parent = game.Players.LocalPlayer.Character["Right Arm"]
    for _, child in ipairs(hit2:GetChildren()) do
        if child:IsA("ParticleEmitter") then -- Check if the child is a ParticleEmitter
            child:Emit(1) -- Emit 20 particles
        end
    end
wait(5)
hit2:Destroy()
hit:Destroy()
game.Lighting.ClockTime = 12.303
end)
